{
  "name": "webpack-wannabe-boilerplate",
  "version": "0.1.0",
  "description": "A working quick to run webpack boilerplate with Stylus and Typescript, with React support. Also includes webpack-dev-server and handles html files decently.",
  "main": "index.js",
  "author": "Arjun Raju Pillai <revarjunr@gmail.com>",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch webpack.config.js --exec 'webpack-dev-server'",
    "build": "NODE_ENV=production webpack -p"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "nodemon": "^1.11.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "util": "^0.10.3",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
